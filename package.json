{
  "name": "tether",
  "version": "0.1.0",
  "private": true,
  "description": "Framework for building AI/ML desktop applications with Tauri, React, and Python",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tether.git"
  },
  "keywords": [
    "tauri",
    "react",
    "python",
    "fastapi",
    "ai",
    "ml",
    "llm",
    "desktop"
  ]
}
