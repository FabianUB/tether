{
  "name": "tether",
  "version": "0.1.0",
  "private": true,
  "description": "Template for building AI/ML desktop applications with Tauri, React, and Python",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tether.git"
  },
  "keywords": [
    "tauri",
    "react",
    "python",
    "fastapi",
    "ai",
    "ml",
    "llm",
    "desktop",
    "template"
  ]
}
